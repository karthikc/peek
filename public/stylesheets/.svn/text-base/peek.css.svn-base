#days {
  font-size: 12px;
  margin-top: 10px;
  text-align: left;
}

#days_input {
  padding-left: 3px;
  margin-left: 5px;
  margin-right: 5px;
}

#fetch {
  margin-left: 10px;
}

#logout {
  float: right;
  font-size: 12px;
  padding-bottom: 10px;
}
#peek_message {
  font-size: 12px;
}
#peek_message .highlight {
  color: #00CC00;
  background-color: #FFFFFF;
}

.room_section {
  border: 1px solid #BBBBBB; 
  padding: 10px;
  margin: 5px;
  clear: both;
}
.room_section h1 {
  margin-bottom: 5px;
  float: left;
}
.people {
  font-size: 12px;
  color: #555555;
  margin-bottom: 5px;
  float: right;
}
.people_label {
  color: #999999;
}
.room_section .adjacent_transcripts {
  clear: both;
}

